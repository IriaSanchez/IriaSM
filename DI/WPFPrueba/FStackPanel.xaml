<Window x:Class="WPFPrueba.FStackPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFPrueba"
        mc:Ignorable="d"
        Title="StackPanel" Height="450" Width="800">
    <DockPanel> 
        
        <StackPanel DockPanel.Dock="Top" Orientation="Vertical">
            <Button  Height="100" Content="Arriba1"/>
            <Button  Width="150" Content="Arriba2"/>
            <Button  Content="Arriba3"/>
            <Button  Height="50" Content="Arriba4"/>
            <Button  Content="Arriba5"/>
        </StackPanel>

        <StackPanel DockPanel.Dock="Bottom" Orientation="Vertical">
            <Button Height="100" Content="Abajo1"/>
            <Button Width="150" Content="Abajo2"/>
            <Button Content="Abajo3"/>
            <Button Width="50" Content="Abajo4"/>
            <Button Content="Abajo5"/>
        </StackPanel>

        <StackPanel  Orientation="Horizontal">
            <Button  Height="100" Content="Boton1111"/>
            <Button  Width="150" Content="Boton2222"/>
            <Button  Content="Boton33333"/>
            <Button  Height="50" Content="Boton44444"/>
            <Button  Content="Boton55555"/>
        </StackPanel>

    </DockPanel>
    
</Window>
